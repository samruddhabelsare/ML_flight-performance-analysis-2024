    "from sklearn.model_selection import train_test_split\n",
    "\n",
    "# Choose features\n",
    "features = [\n",
    "    'month', 'day_of_week', 'crs_dep_time', 'distance',\n",
    "    'carrier_delay', 'weather_delay', 'nas_delay', 'security_delay', 'late_aircraft_delay',\n",
