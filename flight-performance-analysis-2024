    "top_airports = df['origin'].value_counts().nlargest(10).index\n",
    "airport_delay = (\n",
    "    df[df['origin'].isin(top_airports)]\n",
    "    .groupby(['origin', 'is_delayed'])\n",
    "    .size()\n",
    "    .unstack(fill_value=0)\n",
    "    .rename(columns={0: 'On Time', 1: 'Delayed'})\n",
    ")\n",
    "airport_delay = airport_delay.loc[top_airports]  # Keep top 10 order\n",
    "\n",
    "airport_delay.plot(\n",
    "    kind='bar', \n",
    "    stacked=True, \n",
    "    color=['#66b3ff', '#ff6666'], \n",
    "    figsize=(12,6)\n",
    ")\n",
    "plt.title(\"Flight Counts by Delay Status: Top 10 Origin Airports\", fontsize=16, fontweight='bold')\n",
    "plt.xlabel(\"Origin Airport\", fontsize=14)\n",
    "plt.ylabel(\"Number of Flights\", fontsize=14)\n",
    "plt.legend(title=\"Status\")\n",
    "plt.tight_layout()\n",
    "plt.show()\n"
