    "# 1. Target variable\n",
    "df['is_delayed'] = (df['dep_delay'] > 15).astype(int)\n",
    "\n",
    "# 2. Fill missing delay values with 0\n",
    "delay_cols = ['carrier_delay', 'weather_delay', 'nas_delay', 'security_delay', 'late_aircraft_delay']\n",
    "df[delay_cols] = df[delay_cols].fillna(0)\n",
    "\n",
    "# 3. Encode categorical columns\n",
    "cat_cols = ['origin', 'dest', 'op_unique_carrier']\n",
    "for col in cat_cols:\n",
    "    df[col + '_code'] = df[col].astype('category').cat.codes\n",
    "\n",
    "# Check result\n",
    "df[['dep_delay', 'is_delayed', 'origin_code', 'dest_code', 'op_unique_carrier_code']].head()\n"
   ]
